{
  "name": "server",
  "version": "0.0.1",
  "packageManager": "yarn@4.3.1",
  "description": "API boilerplate.",
  "productName": "Fenaka API",
  "private": true,
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "lint": "eslint --fix --ext .ts .",
    "dev": "ts-node-dev --respawn --transpile-only ./index.ts",
    "generate": "drizzle-kit generate --config ./drizzle/config.ts",
    "refresh": "ts-node-dev ./drizzle/migrate --refresh --seed --permission-sync",
    "migrate": "ts-node-dev ./drizzle/migrate",
    "seed": "ts-node-dev ./drizzle/seed",
    "permission-sync": "ts-node-dev ./drizzle/permission-sync"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@as-integrations/koa": "^1.1.1",
    "@faker-js/faker": "^8.4.1",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "axios": "^1.7.2",
    "class-validator": "^0.14.1",
    "dayjs": "^1.11.12",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.32.0",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0",
    "graphql-type-json": "^0.3.2",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-compress": "^5.1.1",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "pg": "^8.12.0",
    "pm2": "^5.4.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "type-graphql": "^2.0.0-rc.2",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/koa": "^2",
    "@types/koa-bodyparser": "^4",
    "@types/koa-static": "^4.0.4",
    "@types/koa__cors": "^5",
    "@types/koa__router": "^12",
    "@types/node": "^20.14.10",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/parser": "^7.16.1",
    "drizzle-kit": "^0.23.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  }
}